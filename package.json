{
  "name": "f1-champians",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@types/node": "^12.20.37",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.20",
    "axios": "^0.24.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "^4.0.3",
    "typescript": "^4.1.6"
  },
  "jest": {
    "modulePathIgnorePatterns": ["cypress"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/utils/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/utils/assetsTransformer.js"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --fix --ext .js src",
    "prettier": "prettier --write 'src/**/*'",
    "scss": "node-sass --output-style compressed -o src/css src/css",
    "watch:css": "onchange 'src/css/**/*.scss' -- npm run scss",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "css-loader": "^0.28.11",
    "cypress": "^9.1.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^26.6.3",
    "node-sass": "^4.10.0",
    "onchange": "^5.1.0",
    "prettier": "^2.5.0",
    "sass-loader": "^7.1.0"
  }
}
